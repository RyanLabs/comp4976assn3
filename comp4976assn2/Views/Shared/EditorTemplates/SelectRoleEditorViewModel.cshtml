@model comp4976assn2.Models.SelectRoleEditorViewModel
@Html.HiddenFor(model => model.RoleName)
<tr>
    <td style="text-align: center">
        @if (Model.RoleName == "Administrator" && Model.RoleCount == 1 && Model.Selected)
        {
            @Html.CheckBoxFor(model => model.Selected, new {@disabled = "disabled"})
        }
        else
        {
            @Html.CheckBoxFor(model => model.Selected)
        }
    </td>
    <td>
        @Html.DisplayFor(model => model.RoleName)
    </td>
</tr>

